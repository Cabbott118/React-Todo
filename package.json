  "bugs": {
    "url": "https://github.com/Cabbott118/React-Todo/issues"
  },
  "homepage": "https://github.com/Cabbott118/React-Todo#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "react-router-dom": "^5.1.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
